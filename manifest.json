{
  "manifest_version": "1",
  "type": "WebApp",
  "meta": {
    "name": "WoT game events sample app",
    "version": "0.0.1",
    "minimum-overwolf-version": "0.92.300.0",
    "author": "Overwolf",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png",
    "description": "WoT game events sample app"
  },
  "data": {
    "start_window": "index",
    "windows": {
      "index": {
        "file": "index.html",
        "transparent": true,
        "clickthrough": true,
        "resizable": false,
        "show_in_taskbar": true,
        "size": {
          "width": 1150,
          "height": 535
        },
        "start_position": {
          "Top": 10,
          "Left": 10
        }
      }
    },
     "game_targeting": {
        "type": "dedicated",
        "game_ids": [
          6365
        ]
      },
    "game_events": [6365],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            6365
          ]
        },
        "start_minimized": false
      }]
}
}
